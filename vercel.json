{
    "builds": [
      {
        "src": "meme_monster/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/staticfiles"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "meme_monster/wsgi.py"
      }
    ]
}